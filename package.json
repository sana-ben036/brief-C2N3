{
  "name": "c2n3-reenforcement-master",
  "version": "1.0.0",
  "description": "Le projet permet d'améliorer vos compétences dans un environnement de travail réel.",
  "main": "index.js",
  "scripts": {
    "sass-w": "node-sass -w --output-style compressed public/scss -o public/css",
    "compil-sass": "node-sass public/scss -o public/css ",
    "prefix-css": "postcss --use autoprefixer -b 'last 5 versions' public/css/style.css -o public/css/style.css",
    "compress-css": "node-sass public/scss public/css --output-style compressed",
    "build-css": "npm-run-all sass-w compil-sass compress-css "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sana-ben036/brief-C2N3.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sana-ben036/brief-C2N3/issues"
  },
  "homepage": "https://github.com/sana-ben036/brief-C2N3#readme",
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
